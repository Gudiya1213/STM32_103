int main(void)
{
  
    HAL_Init();

  SystemClock_Config();
  MX_GPIO_Init();
  MX_USART2_UART_Init();
  while (1)
  {
    /* USER CODE END WHILE */

	  HAL_GPIO_WritePin(GPIOA, GPIO_PIN_5,GPIO_PIN_SET);       // select porta  pin no 5 set as output
          HAL_Delay(1000);
	 
    /* USER CODE BEGIN 3 */
  }
  /* USER CODE END 3 */
}
